{
  "name": "clone-tabnews",
  "version": "1.0.0",
  "description": "Implementação do https://www.tabnews.com.br para o https://curso.dev from paulocod",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest --runInBand",
    "test:watch": "jest --watchAll --runInBand",
    "migration:create": "node-pg-migrate -m infra/migrations create",
    "migration:up": "node-pg-migrate -m infra/migrations --envPath .env.development up",
    "migration:down": "node-pg-migrate -m infra/migrations --envPath .env.development down"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulocod/clone-tabnews.git"
  },
  "keywords": [
    "node",
    "tabnews",
    "clone",
    "curso.dev",
    "paulocod"
  ],
  "author": "Paulo Campos <paulo.campos.dev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paulocod/clone-tabnews/issues"
  },
  "homepage": "https://github.com/paulocod/clone-tabnews#readme",
  "dependencies": {
    "dotenv": "^16.4.4",
    "next": "^13.1.6",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "prettier": "^3.6.2"
  }
}
